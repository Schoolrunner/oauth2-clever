{
    "name": "schoolrunner/oauth2-clever",
    "description": "Clever OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Smoak",
            "email": "csmoak@schoolrunner.org"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "clever"
    ],
    "minimum-stability": "beta",
    "require": {
        "league/oauth2-client": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Schoolrunner\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schoolrunner\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
